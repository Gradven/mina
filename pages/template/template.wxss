  box-sizing: border-box;
  display: flex;
  flex-flow: column nowrap;
  justify-content: space-between;
}

.home_list .item  .card .intro .name {
  width: auto;
  height: 90rpx;
  line-height: 45rpx;
  box-sizing: border-box;
  text-overflow: ellipsis;
  overflow: hidden;
  font-size: 26rpx;
  font-weight: bold;
  white-space: pre-wrap;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  color: #495060;
}

.home_list .item  .card .intro .price {
  color: #1c2438;
