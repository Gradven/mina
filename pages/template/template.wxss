}

.home_list .item .action {
  width: 100%;
  margin-top: 30rpx;
  height: 100rpx;
  display: flex;
  flex-flow: row nowrap;
  background-color: #fcfcfc;
  align-items: center;
}

.home_list .item .action .icon {
  width: 150rpx;
  height: 100%;
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
  justify-content: center;
}

.home_list .item .action .praise {
  flex-grow: 1;
  height: 100%;
  position: relative;
  display: flex;
  flex-flow: row nowrap;
  justify-content: flex-end;
  align-items: center;
  font-size: 24rpx;
  color: #80848f;
  padding-right: 80rpx;
}

.hover {
  width: 150rpx;
  height: 100%;
  background-color: whitesmoke;
  text-align: center;
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
  justify-content: center;
}



.home_list .item .action .praise .heart_ani {
  height: 100rpx;
  width: 110rpx;
  position: absolute;
  right: 0rpx;
  top: 0rpx;
  background-image: url("data:image/png;base64,");
  background-repeat: no-repeat;
  background-size: 3190rpx 100rpx;
  background-position-x: -110rpx;

}



.like-active {
  animation-timing-function: steps(28);
  animation-name: heart-burst;
  animation-duration: 0.8s;
  animation-iteration-count: 1;
  display: inline-block;
  animation-fill-mode: forwards;
}

.heart_liked {
  background-position-x: -3080rpx !important;
}

