.home_list .item  .card {
  margin-left: 68rpx;
  margin-top: 50rpx;
  width: 614rpx;
  height: 250rpx;
  display: flex;
  flex-flow: row nowrap;
  border-radius: 12rpx;
  box-shadow: 0 0 40rpx #dddee1;
}

.home_list .item .card .img_container{
  width: 250rpx;
  height: 100%;
  position: relative;
}

.home_list .item  .card .img {
  width: 250rpx;
  height: 100%;
  border-top-left-radius: 12rpx;
  border-bottom-left-radius: 12rpx;
}

.home_list .item  .card .intro {
  padding: 30rpx 30rpx;
  width: 364rpx;
